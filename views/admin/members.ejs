<section class="content">
    <div class="container-fluid">

        <div class="card card-dark m-3 ">
            <div class="card-header d-flex justify-content-center ">
                <h5 class=" my-auto mr-auto">Data Member</h5>
                <div class="card-tools my-auto">
                    <!-- This will cause the card to collapse when clicked -->

                    <button type="button" class="btn btn-tool"><i class="fa-solid fa-print"></i></button>
                    <button type="button" class="btn btn-tool"><i class="fa-solid fa-arrow-rotate-right"></i></button>

                    <button type="button" class="btn btn-tool" data-card-widget="collapse"><i
                            class="fas fa-minus"></i></button>
                </div>
                <!-- /.card-tools -->
            </div>
            <div class="card-body">
                <div class="d-flex mb-3 justify-content-between">


                    <!--Add Button-->
                    <a name="add" id="" class="btn btn-info mr-2 mr-sm-auto" href="#" data-toggle="modal"
                        data-target="#exampleModal" role="button">
                        <i class="fa fa-plus" aria-hidden="true"></i>
                        <span class="d-none d-sm-inline ml-1">
                            Add Member
                        </span>
                    </a>
                    <!--Modal to Input Member-->
                    <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel"
                        aria-hidden="true">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h5 class="modal-title" id="exampleModalLabel">Add New Member</h5>
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                </div>
                                <div class="modal-body">
                                    <div class="form-group">
                                        <label for="">Name</label>
                                        <input type="text" class="form-control mb-3" name="" id="" placeholder="">
                                        <div class="flex row">
                                            <div class="col-6">
                                                <label for="">Place Of Birth</label>
                                                <input type="text" class="form-control mb-3" name="" id=""
                                                    placeholder="">
                                            </div>
                                            <div class="col-6">
                                                <label for="">Date of Birth</label>
                                                <input type="date" class="form-control mb-3" name="" id=""
                                                    placeholder="">
                                            </div>
                                        </div>
                                        <label for="">Gender</label>
                                        <div class="flex row mb-3 px-2">
                                            <div class="form-check form-check-inline col-2">
                                                <input class="form-check-input" type="radio" name="inlineRadioOptions"
                                                    id="inlineRadio1" value="option1">
                                                <label class="form-check-label" for="inlineRadio1">Male</label>
                                            </div>
                                            <div class="form-check form-check-inline ml-3 col-2">
                                                <input class="form-check-input" type="radio" name="inlineRadioOptions"
                                                    id="inlineRadio2" value="option2">
                                                <label class="form-check-label" for="inlineRadio2">Female</label>
                                            </div>
                                        </div>
                                        <label for="">Address</label>
                                        <input type="text" class="form-control mb-3" name="" id="" placeholder="">
                                        <div class="form-group">
                                            <label for="exampleFormControlFile1">Input Image</label>
                                            <input type="file" accept="image/*" class="form-control-file"
                                                id="exampleFormControlFile1">
                                        </div>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-outline-primary b"
                                        data-dismiss="modal">Close</button>
                                    <button type="button" class="btn btn-primary">Add Member</button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--End Modal-->
                    <!--Search-->
                    <form class="form-inline">
                        <div class="input-group">
                            <input type="search" class="form-control" placeholder="Search"
                                aria-label="Recipient's username" aria-describedby="button-addon2">
                            <div class="input-group-append">
                                <button class="btn btn-outline-secondary" type="submit" id="button-addon2">
                                    <i class="fa-solid fa-magnifying-glass"></i>
                                </button>
                            </div>
                        </div>
                    </form>
                </div>

                <div class="collapse" id="contentId">

                </div>
                <div class="table-responsive ">
                    <table class="table table-striped text-center ">
                        <thead>
                            <tr>
                                <th>No</th>
                                <th style="min-width: 120px;">Name</th>
                                <th style="min-width: 120px;">Place of Birth</th>
                                <th style="min-width: 120px;">Date of Birth</th>
                                <th style="min-width: 90px;">Gender</th>
                                <th style="min-width: 160px;">Address</th>
                                <th style="min-width: 90px;">Gallery</th>
                                <th style="min-width: 144px;">Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td scope="row" class="align-middle">1</td>
                                <td class="align-middle">John Doe</td>
                                <td class="align-middle">New York</td>
                                <td class="align-middle">20-12-2011</td>
                                <td class="align-middle">Male</td>
                                <td class="align-middle">Address road no.11</td>
                                <td>
                                    <button type="button" name="" id="" class="btn btn-info px-3 mx-1">
                                        <i class="fa-solid fa-image-portrait fa-lg my-auto"></i>
                                    </button>
                                </td>
                                <td>
                                    <button type="button" name="" id="" class="btn btn-dark px-3 mx-1">
                                        <i class="fa fa-pencil" aria-hidden="true"></i>
                                    </button>
                                    <button type="button" name="" id="" class="btn btn-danger px-3 mx-1">
                                        <i class="fa fa-trash" aria-hidden="true"></i>
                                    </button>
                                </td>
                            </tr>
                            <tr>
                                <td scope="row" class="align-middle">2</td>
                                <td class="align-middle">Bruce Wayne</td>
                                <td class="align-middle">New Jersey</td>
                                <td class="align-middle">20-12-2011</td>
                                <td class="align-middle">Male</td>
                                <td class="align-middle">Address road no.11</td>
                                <td>
                                    <button type="button" name="" id="" class="btn btn-info px-3 mx-1">
                                        <i class="fa-solid fa-image-portrait fa-lg my-auto"></i>
                                    </button>
                                </td>
                                <td>
                                    <button type="button" name="" id="" class="btn btn-dark px-3 mx-1">
                                        <i class="fa fa-pencil" aria-hidden="true"></i>
                                    </button>
                                    <button type="button" name="" id="" class="btn btn-danger px-3 mx-1">
                                        <i class="fa fa-trash" aria-hidden="true"></i>
                                    </button>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>

            <div class="card-footer d-flex justify-content-between bg-dark rounded-bottom">
                <!--Show Button-->
                <div>
                    <span class="my-auto align-middle d-none d-sm-inline ">Show</span>
                    <div class="btn-group mx-0 mx-sm-2">
                        <button class="btn btn-dark border border-secondary dropdown-toggle px-1 py-0" type="button"
                            id="triggerId" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">10
                        </button>
                        <div class="dropdown-menu dropdown-menu-left bg-dark" aria-labelledby="triggerId">
                            <a class="dropdown-item" href="#">10</a>
                            <a class="dropdown-item" href="#">20</a>
                            <a class="dropdown-item" href="#">50</a>
                            <a class="dropdown-item" href="#">100</a>
                        </div>
                    </div>
                </div>
                <div class="ml-auto">
                    <span class="my-auto align-middle d-none d-sm-inline ">Page</span>
                    <div class="btn-group btn-group-sm" role="group" aria-label="First group">
                        <a class="btn btn-dark "><i class="fa-solid fa-angle-left"></i></a>
                        <a class="btn btn-dark text-white">1</a>
                        <a class="btn btn-dark">2</a>
                        <a class="btn btn-dark disabled">...</a>
                        <a class="btn btn-dark">7</a>
                        <a class="btn btn-dark "><i class="fa-solid fa-angle-right"></i></a>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.card -->
    </div>
</section>